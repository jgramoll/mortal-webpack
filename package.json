{
  "name": "mortal-webpack",
  "description": "webpack configuration for mere mortals",
  "version": "1.1.3",
  "main": "lib/index.js",
  "dependencies": {
    "columnify": "1.5.4",
    "ramda": "0.24.1",
    "stack-trace": "0.0.10"
  },
  "peerDependencies": {
    "happypack": "^4.0.0",
    "html-webpack-plugin": "^2.30.1",
    "webpack": "^4.0.0"
  },
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "eslint": "4.6.1",
    "glob": "7.1.2",
    "happypack": "file:../happypack",
    "html-webpack-plugin": "github:webpack-contrib/html-webpack-plugin",
    "megadoc-preset-js": "6.2.3",
    "megadoc-theme-minimalist": "6.0.0",
    "mocha": "3.5.0",
    "nyc": "11.2.1",
    "sinon": "3.2.1",
    "webpack": "4.0.0"
  },
  "scripts": {
    "docs": "rm -rf docs && megadoc",
